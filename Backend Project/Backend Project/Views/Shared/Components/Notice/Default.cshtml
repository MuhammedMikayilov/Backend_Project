@model HomeVM
<div class="container">
    <div class="row">
        <div class="col-md-6 col-sm-6 col-xs-12">
            <div class="notice-right-wrapper mb-25 pb-25">
                <h3>TAKE A VIDEO TOUR</h3>
                <div class="notice-video">
                    <div class="video-icon video-hover">
                        <a class="video-popup" href="@Model.Notices.ElementAt(0).Video">
                            <i class="zmdi zmdi-play"></i>
                        </a>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-md-6 col-sm-6 col-xs-12">
            <div class="notice-left-wrapper">
                <h3>notice board</h3>
                <div class="notice-left">
                    @foreach (Board board in Model.Notices.ElementAt(0).Boards)
                    {
                        <div class="single-notice-left mb-23 pb-20">
                            <h4>
                                <asp:TextBox ID="TextBox1" runat="server">@board.Time.ToString("dd, MM yyyy")</asp:TextBox>
                            </h4>

                            <p>@Html.Raw(board.Description) </p>
                        </div>
                    }
                </div>
            </div>
        </div>
    </div>
</div>