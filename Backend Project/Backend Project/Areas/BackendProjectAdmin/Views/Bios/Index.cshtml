@model Bios
@{
    ViewData["Title"] = "Index";
}

<table class="table table-striped table-dark">
    <thead>
        <tr>
            <th>Main Phone</th>
            <th>Phone Number</th>
            <th>Phone Number 2</th>
            <th>Address</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@Model.Number</td>
            <td>@Model.Phono1</td>
            <td>@Model.Phone2</td>
            <td>@Model.Address</td>

        </tr>
    </tbody>
</table>

<table class="table table-striped table-dark mt-5">
    <thead>
        <tr>
            <th>Email</th>
            <th>Facebook</th>
            <th>Pinterest</th>
            <th>Vimeo</th>
            <th>Twitter</th>
            <th>Our site</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@Model.EmailUs</td>
            <td>@Model.Facebook</td>
            <td>@Model.Pinterest</td>
            <td>@Model.Vimeo</td>
            <td>@Model.Twitter</td>
            <td>@Model.OurSite</td>
        </tr>
    </tbody>
</table>

<table class="table table-striped table-dark mt-5">
    <thead>
        <tr>
            <th>Header</th>
            <th>Description</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td><img src="~/img/logo/@Model.HeaderLogo" alt="Alternate Text" /></td>
            <td>@Model.Description</td>
        </tr>
    </tbody>
</table>

<div class="row justify-content-center mt-4">
    <a class="btn btn-dark" asp-area="BackendProjectAdmin" asp-controller="Bios" asp-action="Update" asp-route-id="@Model.Id">Update</a>
</div>

